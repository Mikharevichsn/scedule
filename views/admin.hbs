<table class="table table-hover table-dark">
  <thead>
    <tr>
      <th scope="col">Имя</th>
      <th scope="col">Телефон</th>
      <th scope="col">Услуга</th>
      <th scope="col">Начало процедуры</th>
      <th scope="col">Конец процедуры</th>
    </tr>
  </thead>
  <tbody>
    {{#each entriesOfDay}}
    <tr>
      <td>{{this.nameClient}}</td>
      <td>{{this.phone}}</td>
      <td>{{this.serviceId.title}}</td>
      <td class="time">{{this.dateBegin}}</td>
      <td class="time">{{this.dateEnd}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>

<a class="btn" href="/admin">Назад</a>



<script>
  const times = document.querySelectorAll('.time');
  let options = { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric', hour: 'numeric', minute: 'numeric' };
  for (let time of times) {

    time.innerHTML = new Date(time.innerHTML).toLocaleDateString('ru-RU', options)
    { { !-- var date = new Date(times[0].innerHTML); --} }

    { { !--console.log(date.toLocaleDateString('ru-RU', options)); --} }
  }
</script>
